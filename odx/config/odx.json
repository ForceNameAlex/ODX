{
	"id":"odx",
	"name":"Open Dojo Toolkit eXtension",
	"description":"",
	"loaderConfig":{
		"paths":{
			"odx":"../../../../odx"
		}
		
	},
	"dependencies":[
		"dojox/app/widgets/Container",
		"dijit/layout/TabContainer",
		"dijit/layout/ContentPane",
		"dojox/mobile/TabBar",
		"dojox/mobile/TabBarButton"
	],
	"modules":[
		
	],
	"controllers":[
		"dojox/app/controllers/Load",
		"dojox/app/controllers/Transition",
		"dojox/app/controllers/Layout"
	],
	"stores":{
		
	},
	"models":{
		
	},
	"defaultView":"",
	"has":{
		"ie9orLess":{
			"controllers":[
				"dojox/app/controllers/HistoryHash"
			]
			
		},
		"!ie9orLess":{
			"controllers":[
				"dojox/app/controllers/History"
			]
			
		}
		
	},
	"nls":[
		"odx/nls/Global_nls"
	],
	"defaultTransition":"none",
	"defaultView":"header+main",
	"views":{
		"header":{
			"template":"odx/views/header/template/HeaderView.html",
			"constraint":"top"
		},
		"main":{
			"constraint":"center",
			"nls":"odx/views/main/nls/MainView_nls",
			"controller":"odx/views/main/MainView.js",
			"template":"odx/views/main/template/MainView.html",
			"defaultView":"welcome",
			"views":{
				"welcome":{
					"constraint":"center",
					"template":"odx/views/welcome/template/WelcomeView.html"
				},
				"desktop":{
					"constraint":"center",
					"controller":"odx/views/desktop/DesktopView.js",
					"template":"odx/views/desktop/template/DesktopView.html",
					"defaultView": "content",
					"views":{
						"content": {
							"constraint":"center",
							"template": "odx/views/desktop/template/ContentView.html",
						}
					}
				},
				"mobile":{
					"constraint":"center",
					"controller":"odx/views/mobile/MobileView.js",
					"template":"odx/views/mobile/template/MobileView.html"
					
				}
				
			}
			
		}
		
	}
	
}